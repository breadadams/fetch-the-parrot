{
  "name": "fetch-the-parrot",
  "version": "1.0.1",
  "description": "Javascript library for fetching PartyParrot(s)",
  "main": "./dist/index.js",
  "scripts": {
    "test": "mocha ./src/index.test.js -w --compilers js:babel-register",
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist",
    "build:watch": "babel src --out-dir dist --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/breadadams/fetch-the-parrot.git"
  },
  "keywords": [
    "party-parrot",
    "party",
    "parrot"
  ],
  "author": "Brad Adams <hi@breadadams.com> (https://breadadams.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/breadadams/fetch-the-parrot/issues"
  },
  "homepage": "https://github.com/breadadams/fetch-the-parrot#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-babili": "0.1.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "4.1.1",
    "mocha": "3.5.0",
    "rimraf": "2.6.1"
  },
  "babel": {
    "comments": false,
    "presets": [
      "es2015",
      "stage-2",
      "babili"
    ],
    "ignore": [
      "*.test.js"
    ]
  },
  "dependencies": {
    "es6-promise": "4.1.1",
    "isomorphic-fetch": "2.2.1"
  }
}
